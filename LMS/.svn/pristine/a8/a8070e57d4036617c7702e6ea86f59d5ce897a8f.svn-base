DECLARE
  PROCEDURE drop_t(p_tbl VARCHAR2) IS
  BEGIN
    EXECUTE IMMEDIATE 'DROP TABLE '||p_tbl||' CASCADE CONSTRAINTS PURGE';
  EXCEPTION
    WHEN OTHERS THEN
      -- ORA-00942: table or view does not exist -> 무시
