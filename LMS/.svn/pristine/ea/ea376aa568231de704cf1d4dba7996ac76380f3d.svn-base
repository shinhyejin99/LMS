	STUDENT_NO		CHAR(9)		NOT NULL,
	LECTURE_ID			CHAR(15)		NOT NULL,
	ENROLL_STATUS_CD	VARCHAR2(50)	NOT NULL,
	RETAKE_YN			CHAR(1)		NOT NULL,
	STATUS_CHANGE_AT	DATE			NOT NULL,
	AUTO_GRADE		NUMBER(2, 1)	NULL,
	FINAL_GRADE		NUMBER(2, 1)	NULL,
	CHANGE_REASON		VARCHAR2(500)	NULL
);

CREATE TABLE "LCT_ATTROUND" (
	LCT_ROUND	NUMBER		NOT NULL,
	LECTURE_ID	CHAR(15)		NOT NULL,
	ATT_DAY	DATE		NOT NULL,
	QRCODE_FILE_ID	VARCHAR2(50)		NULL
);

CREATE TABLE "STU_TUITION_DETAIL" (
	YEARTERM_CD	CHAR(9)		NOT NULL,
	STUDENT_NO	CHAR(9)		NOT NULL,
	TUITION_ID	CHAR(15)		NOT NULL
);

CREATE TABLE "TIMEBLOCK" (
	TIMEBLOCK_CD	CHAR(6)		NOT NULL,
	WEEKDAY	CHAR(2)		NOT NULL,
	START_HHMM	CHAR(4)		NOT NULL,
	END_HHMM	CHAR(4)		NOT NULL
);

CREATE TABLE "PRF_DEGREE" (
	DEGREE_REGI_ID	VARCHAR2(100)		NOT NULL,
	PROFESSOR_NO	CHAR(8)		NOT NULL,
	DEGREE_NAME	VARCHAR2(50 CHAR)		NOT NULL,
	INSTITUTION	VARCHAR2(50 CHAR)		NOT NULL,
	ACQUIRE_AT	DATE		NOT NULL,
	FINAL_YN	CHAR(1)		NOT NULL
);

CREATE TABLE "PORTAL_NOTICE" (
	NOTICE_ID		CHAR(15)		NOT NULL,
	STAFF_NO		CHAR(7)		NOT NULL,
	NOTICE_TYPE_CD	VARCHAR2(50)	NOT NULL,
	TITLE			VARCHAR2(200)	NOT NULL,
	CONTENT		VARCHAR2(4000)	NOT NULL,
	ATTACH_FILE_ID	VARCHAR2(50)	NULL,
	CREATE_AT		DATE			NOT NULL,
	MODIFY_AT		DATE			NULL,
	DELETE_YN		CHAR(1)		NOT NULL,
	VIEW_CNT		NUMBER		NOT NULL,
	IS_URGENT		CHAR(1 BYTE) 	DEFAULT 'N' NOT NULL
);

CREATE TABLE "STU_OBJECTION" (
	ENROLL_ID	CHAR(15)		NOT NULL,
	OBJECTION_DETAIL	VARCHAR2(4000)		NOT NULL,
	ATTACH_FILE_ID	VARCHAR2(50)		NULL,
	OBJECTION_AT	DATE		NOT NULL,
	REPLY_COMMENT	VARCHAR2(4000)		NULL
);

CREATE TABLE "LCT_ROOM_SCHEDULE" (
	LECTURE_ID	CHAR(15)		NOT NULL,
	PLACE_CD	VARCHAR2(40)		NOT NULL,
	TIMEBLOCK_CD	CHAR(6)		NOT NULL
);

CREATE TABLE "STU_COUNSEL" (
	COUNSEL_ID	CHAR(15)		NOT NULL,
	STUDENT_NO	CHAR(9)		NOT NULL,
	PROFESSOR_NO	CHAR(8)		NOT NULL,
	REQUEST_AT	DATE		NOT NULL,
	COUNSEL_AT	DATE		NOT NULL,
	COUNSEL_YN	CHAR(1)		NOT NULL,
	COUNSEL_DETAIL	VARCHAR2(4000)		NULL
);

CREATE TABLE "EXAM_SUBMIT_DETAIL" (
	LCT_EXAM_ID	CHAR(15)		NOT NULL,
	QUESTION_NO	NUMBER		NOT NULL,
	ENROLL_ID	CHAR(15)		NOT NULL,
	ANSWER	VARCHAR2(4000)		NOT NULL
);

CREATE TABLE "FILE_DETAIL" (
	FILE_ORDER	NUMBER		NOT NULL,
	FILE_ID	VARCHAR2(50)		NOT NULL,
	ORIGIN_NAME	VARCHAR2(400)		NOT NULL,
	EXTENSION	VARCHAR2(100)		NOT NULL,
	SAVE_DIR	VARCHAR2(400)		NOT NULL,
	FILE_SIZE	NUMBER		NOT NULL,
	SAVE_NAME	CHAR(144)		NOT NULL
);

CREATE TABLE "QUESTION_CHOICE" (
	CHOICE_NO	NUMBER		NOT NULL,
	LCT_EXAM_ID	CHAR(15)		NOT NULL,
	QUESTION_NO	NUMBER		NOT NULL,
	CHOICE_CONTENT	VARCHAR2(500)		NOT NULL,
	IS_CORRECT_YN	CHAR(1)		NOT NULL
);

CREATE TABLE "LOGIN_HISTORY" (
	LOGIN_HISTORY_ID	CHAR(15)		NOT NULL,
	USER_ID	CHAR(15)		NOT NULL,
	SUCCESS_YN	CHAR(1)		NOT NULL,
	TRY_AT	DATE		NOT NULL,
	TRY_IP	VARCHAR2(45)		NOT NULL
);

CREATE TABLE "FILES" (
	FILE_ID	VARCHAR2(50)		NOT NULL,
	USER_ID	CHAR(15)			NULL,
	CREATE_AT	DATE	DEFAULT SYSDATE	NOT NULL,
	PUBLIC_YN	CHAR(1)			NOT NULL,
	USING_YN	CHAR(1)			NOT NULL
);

CREATE TABLE "COMMON_CODE_SORT" (
	COMMON_SORT_CD	VARCHAR2(50)	NOT NULL,
	SORT_NAME	VARCHAR2(100)	NOT NULL,
	SORT_DESC	VARCHAR2(1000)	NOT NULL,
	USING_YN	CHAR(1)	NOT NULL
);

CREATE TABLE "COMMON_CODE" (
	COMMON_CD	VARCHAR2(50)	NOT NULL,
	COMMON_SORT_CD	VARCHAR2(50)	NOT NULL,
	CD_NAME	VARCHAR2(100)	NULL,
	CD_DESC	VARCHAR2(1000)	NULL,
	USING_YN	CHAR(1)	NOT NULL
);

CREATE TABLE STU_GRADUATION (
    STUDENT_NO            CHAR(9)        NOT NULL,  -- 학생번호 (STUDENT.STUDENT_NO)
    EXPECTED_YEARTERM_CD  CHAR(9),                  -- 졸업예정학년도학기 (UNIV_YEARTERM.YEARTERM_CD)
    GRADUATE_YEARTERM_CD  CHAR(9),                  -- 졸업학년도학기 (졸업 시 확정)
    GRADUATION_GPA        NUMBER(3,2),              -- 졸업평점 (0.00 ~ 4.50)
    DEGREE_NO             VARCHAR2(50),             -- 학위번호 (발급 시 채번)
    UPDATE_AT             DATE                      -- 수정일자
);

CREATE TABLE STU_EXTRA_MAJOR (
    STUDENT_NO	CHAR(9)		NOT NULL,
    EXTRA_TYPE	VARCHAR2(4)	NOT NULL,
    UNIV_DEPT_CD	VARCHAR2(50)	NOT NULL
);

CREATE TABLE LCT_SECTION_SCORE (
    LECTURE_ID         	CHAR(15)       	NOT NULL,
    GRADE_CRITERIA_CD  	VARCHAR2(50)   	NOT NULL,
    ENROLL_ID          		CHAR(15)       	NOT NULL,
    RAW_SCORE          	NUMBER(5,2)    	NOT NULL
);

CREATE TABLE ENROLL_GPA (
    ENROLL_ID	CHAR(15)		NOT NULL,              -- 수강ID (STU_ENROLL_LCT.FK)
    GPA_CD		VARCHAR2(50)	NOT NULL,              -- 평점 코드 (COMMON_CODE.FK)
    CREATE_AT	DATE			DEFAULT SYSDATE	NOT NULL,  -- 생성일
    UPDATE_AT	DATE			DEFAULT SYSDATE	NOT NULL   -- 수정일
);
