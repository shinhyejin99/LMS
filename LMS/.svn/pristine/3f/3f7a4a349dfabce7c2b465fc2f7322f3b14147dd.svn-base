/* 공통코드 귀찮아서 */
ALTER TABLE LCT_POST
  ADD CONSTRAINT CK_LCT_POST_TYPE
  CHECK (POST_TYPE IN ('NOTICE','MATERIAL'));

/* 학점 0~4.5 */
ALTER TABLE STU_GRADUATION
    ADD CONSTRAINT CK_STU_GRADUATION_GPA
        CHECK (GRADUATION_GPA BETWEEN 0 AND 4.5);

/* 복수전공이나 부전공 */
ALTER TABLE STU_EXTRA_MAJOR
    ADD CONSTRAINT CK_STU_EXTRA_MAJOR_TYPE
        CHECK (EXTRA_TYPE IN ('DUAL', 'SUB'));

/* YNNULL */
ALTER TABLE "APPROVAL"
  ADD CONSTRAINT "CK_APPROVAL_APPROVE_YNNULL"
  CHECK ("APPROVE_YNNULL" IN ('Y','N') OR "APPROVE_YNNULL" IS NULL);

