<select id="findAvailableRoom" resultType="string">
	    SELECT
	        A.PLACE_CD
	    FROM
	        PLACE A
	    WHERE
	        A.PLACE_TYPE_CD = 'ROOM'
	        AND A.CAPACITY >= #{maxCap}

	        <!-- 1. roomPrefix 매개변수를 사용-->
	        AND A.PLACE_CD LIKE #{roomPrefix}

	        AND A.USING_YN = 'Y'

	        <!-- 2. 강의실 시간 중복 체크 -->
	        AND NOT EXISTS (
	            SELECT 1
	            FROM LCT_ROOM_SCHEDULE B
	            WHERE B.PLACE_CD = A.PLACE_CD
	            -- 전달받은 timeBlocks 배열의 모든 요소를 검사
	            AND B.TIMEBLOCK_CD IN
	            <foreach collection="timeBlocks" item="item" open="(" close=")" separator=",">
	                #{item}
	            </foreach>
	        )
	    -- 가장 정원이 작은 강의실부터 배정하여 공간을 효율적으로 사용
	    ORDER BY A.CAPACITY ASC
	    FETCH FIRST 1 ROW ONLY
	</select>

