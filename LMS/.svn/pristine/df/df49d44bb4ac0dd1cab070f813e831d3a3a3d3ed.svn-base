	<select id="isProfessorBusy" resultType="int">
	    SELECT
	        COUNT(T2.LECTURE_ID)  -- 겹치는 강의 건수 계산
	    FROM
	        LECTURE T1  -- T1: 이미 배정된 강의 정보를 가져오기 위함
	    INNER JOIN
	        LCT_ROOM_SCHEDULE T2 ON T1.LECTURE_ID = T2.LECTURE_ID
	    WHERE
	        -- 1. 중복 확인 대상 교수 지정
	        T1.PROFESSOR_NO = #{professorNo}
	        -- 2. 해당 교수가 배정받으려는 시간 블록들 중 하나라도 사용 중인지 확인
	        AND T2.TIMEBLOCK_CD IN
	        <foreach collection="timeBlocks" item="item" open="(" close=")" separator=",">
	            #{item}
	        </foreach>
	</select>

